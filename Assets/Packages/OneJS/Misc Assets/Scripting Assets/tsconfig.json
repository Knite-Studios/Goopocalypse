{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "*": [
                "ScriptLib/3rdparty/*",
                "ScriptLib/*",
                "Addons/*",
                "Modules/*",
                "*"
            ]
        },
        "strictNullChecks": true,
        "skipLibCheck": true,
        "removeComments": true,
        "jsx": "react",
        "jsxFactory": "h",
        "target": "es5",
        "module": "commonjs",
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "ES6", "es2017"
        ]
    }
}
